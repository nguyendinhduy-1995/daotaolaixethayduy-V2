{
  "name": "thayduy-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "seed:admin": "ts-node --transpile-only scripts/seed-admin.ts",
    "seed:templates": "ts-node --transpile-only scripts/seed-templates.ts",
    "seed:tuition": "ts-node --transpile-only scripts/seed-tuition-plans.ts",
    "dev": "node scripts/dev.mjs",
    "dev:stable": "node scripts/dev-stable.mjs",
    "build": "next build",
    "start": "node scripts/start.mjs",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "db:up": "docker compose up -d postgres redis",
    "db:down": "docker compose down",
    "db:reset": "prisma migrate reset --force",
    "db:migrate": "prisma migrate deploy",
    "check:schema": "node scripts/check-schema.mjs",
    "db:seed": "prisma db seed",
    "audit:permissions": "ts-node --transpile-only scripts/audit-route-permissions.ts",
    "prisma:generate": "prisma generate",
    "prisma:validate": "prisma validate",
    "verify": "node scripts/verify-gate.mjs",
    "worker:outbound:dry": "bash scripts/worker-outbound.sh dry",
    "worker:outbound": "bash scripts/worker-outbound.sh run",
    "log:session": "node scripts/log-session.mjs",
    "test:e2e:hardening": "node scripts/run-e2e.mjs tests/hardening-v2.spec.ts"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.3",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "prisma": "^7.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}